<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="Gpen Editor"
            extensions="gpen"
            icon="icons/gpen16.png"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            class="kz.greepto.gpen.editors.gpen.GpenEditor"
            id="kz.greepto.gpen.editors.GpenEditor">
      </editor>
      <editor
            class="kz.greepto.gpen.editors.gdoc.GdocEditor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            extensions="gdoc"
            icon="icons/gdoc16.png"
            id="kz.greepto.gpen.editors.GdocEditor"
            name="Gdoc Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="kz.greepto.gpen"
            name="Gpen">
      </category>
      <view
            category="kz.greepto.gpen"
            class="kz.greepto.gpen.views.gpen.GpenPropertyView"
            icon="icons/props.gif"
            id="kz.greepto.gpen.views.gpen.GpenPropertyView"
            name="Gpen Properties">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="kz.greepto.gpen.views.gpen.GpenPropertyView"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.ProblemView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>

</plugin>
